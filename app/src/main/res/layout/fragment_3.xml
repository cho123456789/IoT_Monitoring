<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragment_InOutput">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.258"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="347dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="69dp"
                    android:background="#344955"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Digital IN / OUT"
                    android:textColor="#FFFFFF"
                    android:textSize="35sp" />

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="51dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/input_channel"
                        android:layout_width="146dp"
                        android:layout_height="28dp"
                        android:layout_gravity="clip_horizontal|center_horizontal|center_vertical"
                        android:layout_marginStart="20dp"
                        android:background="@drawable/txt_shape_input"
                        android:gravity="clip_horizontal|center"
                        android:text="Input"
                        android:textColor="@color/black"
                        android:textSize="20sp" />

                    <Space
                        android:layout_width="11dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <TextView
                        android:id="@+id/output_channel"
                        android:layout_width="174dp"
                        android:layout_height="28dp"
                        android:layout_gravity="clip_horizontal|center_horizontal|center_vertical"
                        android:layout_marginEnd="35dp"
                        android:background="@drawable/txt_shape_input"
                        android:gravity="clip_horizontal|center"
                        android:text="Onput"
                        android:textColor="@color/black"
                        android:textSize="20sp" />


                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="18dp" />

                <LinearLayout
                    android:id="@+id/lin0"
                    android:layout_width="match_parent"
                    android:layout_height="152dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="115dp"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="187dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/gray0"
                                android:layout_width="111dp"
                                android:layout_height="94dp"
                                android:layout_marginLeft="30sp"
                                android:src="@drawable/gray" />

                            <TextView
                                android:id="@+id/textView11"
                                android:layout_width="112dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30sp"
                                android:background="@drawable/txt_shape_input"
                                android:gravity="center_horizontal|center_vertical"
                                android:text="0channel"
                                android:textColor="#090909"
                                android:textSize="15sp" />

                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/txt_off_0"
                            android:layout_width="54dp"
                            android:layout_height="39dp"
                            android:background="@drawable/txt_input_off"
                            android:gravity="center_horizontal|center_vertical"
                            android:text="OFF"
                            android:textColor="#F6F6F6" />

                        <Switch
                            android:id="@+id/switch_0"
                            android:layout_width="83dp"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal|center_vertical"
                            android:switchMinWidth="70dp"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <TextView
                            android:id="@+id/txt_on_0"
                            android:layout_width="54dp"
                            android:layout_height="39dp"
                            android:layout_marginEnd="20sp"
                            android:background="@drawable/txt_shape"
                            android:gravity="center_horizontal|center_vertical"
                            android:text="ON"
                            android:textColor="#FFFFFF" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lin1"
                    android:layout_width="match_parent"
                    android:layout_height="132dp"
                    android:layout_gravity="clip_horizontal|center_horizontal|center_vertical"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="187dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/gray1"
                            android:layout_width="111dp"
                            android:layout_height="91dp"
                            android:layout_marginLeft="30dp"
                            android:src="@drawable/gray" />

                        <TextView
                            android:id="@+id/txt_ch_1"
                            android:layout_width="112dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30sp"
                            android:background="@drawable/txt_shape_input"
                            android:gravity="center_horizontal|center_vertical"
                            android:text="1channel"
                            android:textColor="#000000"
                            android:textSize="15sp" />

                    </LinearLayout>


                    <TextView
                        android:id="@+id/txt_on_2"
                        android:layout_width="54dp"
                        android:layout_height="39dp"
                        android:background="@drawable/txt_input_off"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="OFF"
                        android:textColor="#F6F6F6" />

                    <Switch
                        android:id="@+id/switch_1"
                        android:layout_width="83dp"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal|center_vertical"
                        android:minHeight="100dp"
                        android:switchMinWidth="70dp" />


                    <TextView
                        android:id="@+id/txt_off_"
                        android:layout_width="54dp"
                        android:layout_height="39dp"
                        android:background="@drawable/txt_shape"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="ON"
                        android:textColor="#F6F6F6" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lin2"
                    android:layout_width="match_parent"
                    android:layout_height="132dp"
                    android:gravity="clip_horizontal"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="187dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/gray2"
                            android:layout_width="111dp"
                            android:layout_height="91dp"
                            android:layout_marginLeft="30dp"
                            android:src="@drawable/gray" />

                        <TextView
                            android:id="@+id/txt_ch_2"
                            android:layout_width="112dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30sp"
                            android:background="@drawable/txt_shape_input"
                            android:gravity="center_horizontal|center_vertical"
                            android:text="2channel"
                            android:textColor="#000000"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/txt_off_3"
                        android:layout_width="54dp"
                        android:layout_height="39dp"
                        android:background="@drawable/txt_input_off"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="OFF"
                        android:textColor="#F6F6F6" />

                    <Switch
                        android:id="@+id/switch_2"
                        android:layout_width="83dp"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal|center_vertical"
                        android:minHeight="100dp"
                        android:switchMinWidth="70dp" />

                    <TextView
                        android:id="@+id/txt_on_3"
                        android:layout_width="54dp"
                        android:layout_height="39dp"
                        android:background="@drawable/txt_shape"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="ON"
                        android:textColor="#F6F6F6" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lin3"
                    android:layout_width="match_parent"
                    android:layout_height="148dp"
                    android:gravity="clip_horizontal"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="187dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/gray3"
                            android:layout_width="111dp"
                            android:layout_height="91dp"
                            android:layout_marginLeft="30dp"
                            android:src="@drawable/gray" />


                        <TextView
                            android:id="@+id/txt_ch_3"
                            android:layout_width="114dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30sp"
                            android:background="@drawable/txt_shape_input"
                            android:gravity="center_horizontal|center_vertical"
                            android:text="3channel"
                            android:textColor="#000000"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/txt_off_4"
                        android:layout_width="54dp"
                        android:layout_height="39dp"
                        android:background="@drawable/txt_input_off"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="OFF"
                        android:textColor="#F6F6F6" />

                    <Switch
                        android:id="@+id/switch_3"
                        android:layout_width="83dp"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal|center_vertical"
                        android:minHeight="100dp"
                        android:switchMinWidth="70dp" />

                    <TextView
                        android:id="@+id/txt_on_4"
                        android:layout_width="54dp"
                        android:layout_height="39dp"
                        android:background="@drawable/txt_shape"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="ON"
                        android:textColor="#F6F6F6" />

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="117dp" />

    </LinearLayout>

</FrameLayout>